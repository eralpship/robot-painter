    "@tanstack/react-router-devtools": "^1.114.3",
    "@tanstack/router-plugin": "^1.114.3",
    "leva": "^0.10.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-moveable": "^0.56.0",
    "react-rnd": "^10.5.2",
    "three": "^0.175.0",
    "uuid": "^13.0.0"
